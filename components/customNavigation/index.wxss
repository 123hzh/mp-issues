.fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 9999;
}

.custom-nav {
  display: flex;
  flex-direction: column;
}

.nav {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.tool-bar {
  flex: 0 0 64rpx;
}

.main {
  flex: 1;
}

.back {
  display: inline-block;
  height: 32rpx;
  width: 32rpx;
  margin-left: 30rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: contain;
}
